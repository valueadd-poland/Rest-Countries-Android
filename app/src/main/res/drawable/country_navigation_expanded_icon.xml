    <item>
        <shape android:shape="oval">
            <solid android:color="@color/blackAlpha50" />
            <size
                android:width="@dimen/country_navigation_icon_expanded_size"
                android:height="@dimen/country_navigation_icon_expanded_size" />
        </shape>
    </item>
    <item>
        <inset
